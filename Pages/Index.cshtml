@page
@model IndexModel
@{
    ViewData["Title"] = "onremote - Remote Support neu definiert.";
}

<div class="row">
    <div class="col-xxl-5 col-xl-6 col-lg-8">
        <div class="hero__content-2">
            <h2 class="hero__title-2">Remote Support neu definiert.</h2>
            <p>Video Telefonie mit Augemented Reality und Künstlicher Intelligenz. Damit Du schneller mit Partnern, Lieferanten und Kunden kommunizierst.</p>
            <button class="btn fifth">Button 5</button>
        </div>
    </div>
    <div class="col-xxl-6 offset-xxl-1 col-xl-6">
        <div class="hero__thumb-2 mt-80">
            <div class="hero__thumb-inner p-relative ml-90">
                <img class="hero-2-thumb" src="/img/3d-boxes.png" />
                <img class="hero-2-girl" src="/img/woman-phone.png" />
                <img class="hero-2-thumb-sm" src="/img/pfeil.png">
                <img class="hero-2-thumb-sm-2" src="/img/man-laptop.png" />
                <img class="hero-2-thumb-sm-3" src="/img/circle.png"/>
                <img class="hero-2-leaf" src="/img/hero-2-leaf.png" />
            </div>
        </div>
    </div>
</div>
